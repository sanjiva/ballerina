h1:first-child, h2:first-child, h3:first-child, h4:first-child, h5:first-child {
    margin-top: 20px;
    margin-bottom: 10px;
}

/*-------------------
        Text
--------------------*/
h3 {
    font-weight : 400;
}

p {
  margin        : @paragraphMargin;
  line-height   : @paragraphLineHeight;
}

p:first-child {
  margin-top    : 0em;
}

p:last-child {
  margin-bottom : 0em;
}

body{
  font-weight: 300;
}

/*******************************
          Highlighting
*******************************/
/* Site */
::-webkit-selection {
  background-color: #f1f1f1;
  color: rgba(0, 0, 0, 0.87);
}
::-moz-selection {
  background-color: #f1f1f1;
  color: rgba(0, 0, 0, 0.87);
}
::selection {
  background-color: #f1f1f1;
  color: rgba(0, 0, 0, 0.87);
}