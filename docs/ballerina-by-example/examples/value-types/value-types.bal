function main (string[] args) {
    //Variables defined without an initial value are zero-valued. For example, the zero value for an int is 0.
    int i;
    println(i);

    float f = 20.0;
    println(f);

    //The zero value of a string is null.
    string s;
    println(s);

    boolean b = true;
    println(b);
}
