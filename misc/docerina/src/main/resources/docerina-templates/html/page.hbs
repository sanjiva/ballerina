                                                            <td class="cAPI-table-name"></td>
                                                            <td class="cAPI-table-type"><a href="{{href}}">{{dataType}}</a></td>
                                                            <td class="cAPI-table-default"></td>
                                                            <td class="cAPI-table-desc">{{{description}}}</td>
                                                        </tr>
                                                    {{/each}}
                                                </table>
                                            {{/if}} {{/if}}
                                            <!-- Struct-binded functions grouped together -->
                                            {{#if isStruct}}
                                                <h2 id="{{name}}">
                                                    <span class="{{icon}} sm-icon"></span> public object {{name}}
                                                </h2>
                                                {{#if description}}
                                                    <p>
                                                        <span class="fw-right gray xs-icon"></span>{{{description}}}
                                                    </p>
                                                {{/if}} {{#if fields}}
