<!DOCTYPE HTML>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="docerina-theme/libs/bootstrap-3.0.0/css/bootstrap.min.css">

    <!-- Nanoscroller CSS -->
    <link rel="stylesheet" href="docerina-theme/css/nanoscroller.css">

    <!-- Ballerina font CSS -->
    <link rel="stylesheet" href="docerina-theme/css/font-ballerina/css/font-ballerina.css">

    <!-- Roboto font CSS -->
    <link rel="stylesheet" href="docerina-theme/css/roboto.css">

    <!-- custom CSS -->
    <link rel="stylesheet" href="docerina-theme/css/main.css">

    <!-- Jquery JS -->
    <script src="docerina-theme/js/jquery.min.js"></script>

    <!-- Bootstrap JS -->
    <script src="docerina-theme/libs/bootstrap-3.0.0/js/bootstrap.min.js"></script>

    <!-- Nanoscroller JS -->
    <script src="docerina-theme/js/jquery.nanoscroller.min.js"></script>

    <!-- Docerina JS -->
    <script src="docerina-theme/js/docerina.js"></script>

    <title>Ballerina API Documentation</title>
</head>
<body>
<!-- Fixed navbar -->
<div id="header" class="navbar navbar-fixed-top">
    <div class="container-full">
        <a href="index.html"><strong>Ballerina</strong> API Documentation</a>
    </div>
</div>
<div class="container-full">
    <div class="row docerina-content">
        <div id="packageList" class="col-sm-3 col-lg-2">
            <nav class="navbar navbar-default navbar-fixed-side nano">
                <div class="container nano-content">
                    <div class="navbar-header">
                        <h3><span class="bold">Ballerina</span> Packages</h3>
                    </div>
                    <div class="collapse navbar-collapse package-list-wrapper">
                        {{#each links}}
                            <ul class="nav navbar-nav package-name-ul {{#if active}}active-package{{/if}}">
                                <li>
                                    <!--Package Name-->
                                    {{#if active}}
                                        {{#if caption.isPackageName}}
                                            <a>{{caption.prefix}}<span class="bold">{{caption.suffix}}</a>
                                        {{/if}}
                                        {{#if caption.isStaticCaption}}
                                            <a><span class="bold">{{caption.value}}</span></a>
                                        {{/if}}
                                        <div class="constructs-wrapper">
                                            <!--Construct of Package-->
                                            <ul class="constructs-ul">
                                                {{#each constructs}}
                                                    <li>
                                                        <a href="#{{name}}">
                                                            <div class="construct-icon">
                                                                <span class="{{icon}}"> </span>
                                                            </div>
                                                            <div class="construct-name">
                                                                {{name}}
                                                                {{#if isFunction}}
                                                                    ( {{join parameters " , "}} )
                                                                    {{#if returnParams}}
                                                                    ( {{join returnParams " , "}} )
                                                                    {{/if}}
                                                                {{/if}}
                                                                {{#if isConnector}}
                                                                    ({{#each parameters}}{{dataType}} {{name}}{{#unless
                                                                            @last}}
                                                                    ,{{/unless}}{{/each}})
                                                                {{/if}}
                                                            </div>
                                                        </a>
                                                    </li>
                                                    {{#each children}}
                                                        <ul class="construct-inner-items-ul">
                                                            <li>
                                                                <a href="#{{../name}}-{{name}}">
                                                                    <div class="construct-icon">
                                                                        <span class="{{icon}}"> </span>
                                                                    </div>
                                                                    <div class="construct-name">
                                                                        {{name}}
                                                                        ( {{join parameters " , "}} )
                                                                        {{#if returnParams}}
                                                                        ( {{join returnParams " , "}} )
                                                                        {{/if}}
                                                                    </div>
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    {{/each}}
                                                {{/each}}
                                            </ul>
                                        </div>
                                    {{else}}
                                        <a href="{{href}}.html">
                                            {{#if caption.isPackageName}}
                                                {{caption.prefix}}<span class="bold">{{caption.suffix}}</span>
                                            {{/if}}
                                            {{#if caption.isStaticCaption}}
                                                <span class="bold">{{caption.value}}</span>
                                            {{/if}}
                                        </a>
                                    {{/if}}
                                </li>
                            </ul>
                        {{/each}}
                    </div>
                </div>
            </nav>
        </div>
        <!-- Main Content on the right-->
        <div class="col-sm-9 col-lg-10 content-wrapper main-wrapper">
            <div id="content" class="nano">
                <div class="nano-content">
                    <h3 class="thin-text padding-bottom">
                        <span class="bold">{{heading.value}}</span>
                        {{#if heading.isPackageName}}
                             package
                        {{/if}}
                    </h3>
                    {{#each constructs}}
                        <div class="construct-wrapper">
                            <!--Primitive Types-->
                            {{#if isPrimitiveType}}
                                <h4 id="{{name}}">
                                    <span class="{{icon}} sm-icon"></span>
                                    {{name}}
                                </h4>
                                {{#if description}}
                                    <p>
                                        <span class="fw-right gray xs-icon"></span>{{description}}
                                    </p>
                                {{/if}}
                                {{#if children}}
                                    <div class="function-wrapper">
                                        <ul>
                                            {{#each children}}
                                                <li>
                                                    <h4 id="{{../name}}-{{name}}">
                                                        <span class={{icon}}></span>
                                                        <span class="thin-text light-gray">&lt; {{../name}} &gt;</span>
                                                        {{name}}
                                                        ( {{join parameters " , "}} )
                                                        {{#if returnParams}}
                                                        ( {{join returnParams " , "}} )
                                                        {{/if}}
                                                    </h4>
                                                    {{#if description}}
                                                        <p>
                                                            <span class="fw-right gray xs-icon"></span>{{description}}
                                                        </p>
                                                    {{/if}}
                                                    {{#if parameters}}
                                                        <p>
                                                            <label class="table-header-text">Parameters:</label>
                                                        </p>
                                                        <table>
                                                            <tr>
                                                                <th class="col-md-3">Parameter Name</th>
                                                                <th class="col-md-2">Data Type</th>
                                                                <th class="col-md-7">Description</th>
                                                            </tr>
                                                            {{#each parameters}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                            {{/each}}
                                                        </table>
                                                    {{/if}}
                                                    {{#if returnParams}}
                                                        <p>
                                                            <label class="table-header-text">Return Parameters:</label>
                                                        </p>
                                                        <table>
                                                            <tr>
                                                                <th class="col-md-3">Return Variable</th>
                                                                <th class="col-md-2">Data Type</th>
                                                                <th class="col-md-7">Description</th>
                                                            </tr>
                                                            {{#each returnParams}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                            {{/each}}
                                                        </table>
                                                    {{/if}}
                                                </li>
                                            {{/each}}
                                        </ul>
                                    </div>
                                {{/if}}
                            {{/if}}
                            <!-- Function -->
                            {{#if isFunction}}
                                <h4 id="{{name}}">
                                    <span class="{{icon}} sm-icon"> </span>
                                    public function {{name}}
                                    ( {{join parameters " , "}} )
                                    {{#if returnParams}}
                                    ( {{join returnParams " , "}} )
                                    {{/if}}
                                </h4>
                                {{#if description}}
                                    <p>
                                        <span class="fw-right gray xs-icon"></span>{{description}}
                                    </p>
                                {{/if}}
                                {{#if parameters}}
                                    <p>
                                        <label class="table-header-text">Parameters:</label>
                                    </p>
                                    <table>
                                        <tr>
                                            <th class="col-md-3">Parameter Name</th>
                                            <th class="col-md-2">Data Type</th>
                                            <th class="col-md-7">Description</th>
                                        </tr>
                                        {{#each parameters}}
                                            <tr>
                                                <td class="col-md-3">{{name}}</td>
                                                <td class="col-md-2">{{dataType}}</td>
                                                <td class="col-md-7">{{description}}</td>
                                            </tr>
                                        {{/each}}
                                    </table>
                                {{/if}}
                                {{#if returnParams}}
                                    <p>
                                        <label class="table-header-text">Return Parameters:</label>
                                    </p>
                                    <table>
                                        <tr>
                                            <th class="col-md-3">Return Variable</th>
                                            <th class="col-md-2">Data Type</th>
                                            <th class="col-md-7">Description</th>
                                        </tr>
                                        {{#each returnParams}}
                                            <tr>
                                                <td class="col-md-3">{{name}}</td>
                                                <td class="col-md-2">{{dataType}} </td>
                                                <td class="col-md-7">{{description}}</td>
                                            </tr>
                                        {{/each}}
                                    </table>
                                {{/if}}
                            {{/if}}
                            <!-- Struct-binded functions grouped together -->
                            {{#if isStruct}}
                                <h4 id="{{name}}">
                                    <span class="{{icon}} sm-icon"></span>
                                    public struct {{name}}
                                </h4>
                                {{#if description}}
                                    <p>
                                        <span class="fw-right gray xs-icon"></span>{{description}}
                                    </p>
                                {{/if}}
                                {{#if fields}}
                                    <p>
                                        <label class="table-header-text">Fields:</label>
                                    </p>
                                    <table>
                                        <tr>
                                            <th class="col-md-3">Field Name</th>
                                            <th class="col-md-2">Data Type</th>
                                            <th class="col-md-5">Description</th>
                                            <th class="col-md-2">Default Value</th>
                                        </tr>
                                        {{#each fields}}
                                            <tr>
                                                <td class="col-md-3">{{name}}</td>
                                                <td class="col-md-2">{{dataType}}</td>
                                                <td class="col-md-5">{{description}}</td>
                                                <td class="col-md-2">{{defaultValue}}</td>
                                            </tr>
                                        {{/each}}
                                    </table>
                                {{/if}}
                                {{#if children}}
                                    <div class="function-wrapper">
                                        <ul>
                                            {{#each children}}
                                                <li>
                                                    <h4 id="{{../name}}-{{name}}">
                                                        <span class={{icon}}></span>
                                                        <span class="thin-text light-gray">&lt; {{../name}} &gt;</span>
                                                        {{name}}
                                                        ( {{join parameters " , "}} )
                                                        {{#if returnParams}}
                                                        ( {{join returnParams " , "}} )
                                                        {{/if}}
                                                    </h4>
                                                    {{#if description}}
                                                        <p>
                                                            <span class="fw-right gray xs-icon"></span>{{description}}
                                                        </p>
                                                    {{/if}}
                                                    {{#if parameters}}
                                                        <p>
                                                            <label class="table-header-text">Parameters:</label>
                                                        </p>
                                                        <table>
                                                            <tr>
                                                                <th class="col-md-3">Parameter Name</th>
                                                                <th class="col-md-2">Data Type</th>
                                                                <th class="col-md-7">Description</th>
                                                            </tr>
                                                            {{#each parameters}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                            {{/each}}
                                                        </table>
                                                    {{/if}}
                                                    {{#if returnParams}}
                                                        <p>
                                                            <label class="table-header-text">Return Parameters:</label>
                                                        </p>
                                                        <table>
                                                            <tr>
                                                                <th class="col-md-3">Return Variable</th>
                                                                <th class="col-md-2">Data Type</th>
                                                                <th class="col-md-7">Description</th>
                                                            </tr>
                                                            {{#each returnParams}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                            {{/each}}
                                                        </table>
                                                    {{/if}}
                                                </li>
                                            {{/each}}
                                        </ul>
                                    </div>
                                {{/if}}
                            {{/if}}
                            <!-- Connectors and its actions -->
                            {{#if isConnector}}
                                <h4 id="{{name}}">
                                    <span class="{{icon}} sm-icon"></span>
                                    public connector {{name}} ( {{join parameters " , "}} )
                                </h4>
                                {{#if description}}
                                    <p>
                                        <span class="fw-right gray xs-icon"></span>{{description}}
                                    </p>
                                {{/if}}
                                {{#if parameters}}
                                    <p>
                                        <label class="table-header-text">Parameters:</label>
                                    </p>
                                    <table>
                                        <tr>
                                            <th class="col-md-4">Parameter Name</th>
                                            <th class="col-md-2">Data Type</th>
                                            <th class="col-md-6">Description</th>
                                        </tr>
                                        {{#each parameters}}
                                            <tr>
                                                <td class="col-md-3">{{name}}</td>
                                                <td class="col-md-2">{{dataType}}</td>
                                                <td class="col-md-7">{{description}}</td>
                                            </tr>
                                        {{/each}}
                                    </table>
                                {{/if}}
                                {{#if children}}
                                    <div class="function-wrapper">
                                        <h4>Actions:</h4>
                                        <ul>
                                            {{#each children}}
                                                <li>
                                                    <h4 id="{{../name}}-{{name}}">
                                                        <span class={{icon}}></span>
                                                        <span class="thin-text light-gray">< {{../name}} ></span>
                                                        {{name}}
                                                        ( {{join parameters " , "}} )
                                                        {{#if returnParams}}
                                                        ( {{join returnParams " , "}} )
                                                        {{/if}}
                                                    </h4>
                                                    {{#if description}}
                                                        <p>
                                                            <span class="fw-right gray xs-icon"></span>{{description}}
                                                        </p>
                                                    {{/if}}
                                                    {{#if parameters}}
                                                        <p>
                                                            <label class="table-header-text">Parameters:</label>
                                                        </p>
                                                        <table>
                                                            <tr>
                                                                <th class="col-md-3">Parameter Name</th>
                                                                <th class="col-md-2">Data Type</th>
                                                                <th class="col-md-7">Description</th>
                                                            </tr>
                                                            {{#each parameters}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                            {{/each}}
                                                        </table>
                                                    {{/if}}
                                                    {{#if returnParams}}
                                                        <p>
                                                            <label class="table-header-text">Return Parameters:</label>
                                                        </p>
                                                        <table>
                                                            <tr>
                                                                <th class="col-md-3">Return Variable</th>
                                                                <th class="col-md-2">Data Type</th>
                                                                <th class="col-md-7">Description</th>
                                                            </tr>
                                                            {{#each returnParams}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                            {{/each}}
                                                        </table>
                                                    {{/if}}
                                                </li>
                                            {{/each}}
                                        </ul>
                                    </div>
                                {{/if}}
                            {{/if}}
                            <!-- Enums-->
                            {{#if isEnum}}
                                <h4 id="{{name}}">
                                    <span class="{{icon}} sm-icon"></span>
                                    public enum {{name}}
                                </h4>
                                {{#if description}}
                                    <p>
                                        <span class="fw-right gray xs-icon"></span>{{description}}
                                    </p>
                                {{/if}}
                                {{#if enumerators}}
                                    <p>
                                        <label class="table-header-text">Enumerators:</label>
                                    </p>
                                    <table>
                                        <tr>
                                            <th class="col-md-3">Name</th>
                                            <th class="col-md-9">Description</th>
                                        </tr>
                                        {{#each enumerators}}
                                            <tr>
                                                <td class="col-md-3">{{name}}</td>
                                                <td class="col-md-9">{{description}}</td>
                                            </tr>
                                        {{/each}}
                                    </table>
                                {{/if}}
                            {{/if}}
                            <!-- Annotations-->
                            {{#if isAnnotation}}
                                <h4 id="{{name}}">
                                    <span class="{{icon}} sm-icon"></span>
                                    public annotation {{name}}
                                </h4>
                                {{#if description}}
                                    <p>
                                        <span class="fw-right gray xs-icon"></span>{{description}}
                                    </p>
                                {{/if}}
                                {{#if attributes}}
                                    <p>
                                        <label class="table-header-text">Attributes:</label>
                                    </p>
                                    <table>
                                        <tr>
                                            <th class="col-md-3">Attribute Name</th>
                                            <th class="col-md-2">Data Type</th>
                                            <th class="col-md-7">Description</th>
                                        </tr>
                                        {{#each attributes}}
                                            <tr>
                                                <td class="col-md-3">{{name}}</td>
                                                <td class="col-md-2">{{dataType}}</td>
                                                <td class="col-md-7">{{description}}</td>
                                            </tr>
                                        {{/each}}
                                    </table>
                                {{/if}}
                            {{/if}}
                            <!-- Global Variables-->
                            {{#if isGlobalVariable}}
                                <h4 id="{{name}}">
                                    <span class="{{icon}} sm-icon"></span>
                                    {{name}}
                                </h4>
                                <table>
                                    <tr>
                                        <th class="col-md-3">Attribute Name</th>
                                        <th class="col-md-2">Data Type</th>
                                        <th class="col-md-7">Description</th>
                                    </tr>
                                    <tr>
                                        <td class="col-md-3">{{name}}</td>
                                        <th class="col-md-2">{{dataType}}</th>
                                        <td class="col-md-7">{{description}}</td>
                                    </tr>
                                </table>
                            {{/if}}
                        </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</div>
</body>
</html>