                                                <caption>Endpoints Summary</caption>
                                                <tr>
                                                    <th class="cSummary-table-type">Endpoint</th>
                                                    <th class="cSummary-table-desc">Description</th>
                                                </tr>
                                                {{#each constructs}}
                                                    {{#if isConnector}}
                                                        <tr>
                                                            <td class="cSummary-table-type"><a href="#{{name}}">{{name}}</a></td>
                                                            <td class="cSummary-table-desc">{{{description}}}</td>
                                                        </tr>
                                                    {{/if}}
                                                {{/each}}
                                            </table>
                                            <p/><p/>
                                            <table>
                                                <caption>Functions Summary</caption>
                                                <tr>
                                                    <th class="cSummary-table-type">Return Type</th>
                                                    <th class="cSummary-table-desc">Function and Description</th>
                                                </tr>
                                                {{#each constructs}}
                                                    {{#if isFunction}}
                                                        <tr>
                                                            <td class="cSummary-table-type">{{#if returnParams}}{{join returnParams ", "}}{{/if}}</td>
                                                            <td class="cSummary-table-desc"><a href="#{{name}}">{{name}}({{join parameters ", "}})</a></br>{{{description}}}</td>
                                                        </tr>
                                                    {{/if}}
                                                {{/each}}
                                            </table>
                                            <p/><p/>
                                            <!-- Type Definitions-->
                                            <h2 id="TypeDefinitions">
                                                <span class="fw-constant sm-icon"></span> Type Definitions
                                            </h2>
                                            <table>
                                                <tr>
                                                    <th class="cAPI-table-name">Type</th>
                                                    <th class="cAPI-table-type">Values</th>
                                                    <th class="cAPI-table-desc">Description</th>
                                                </tr>
                                                {{#each constructs}}
                                                    {{#if isType}}
                                                        <tr>
                                                            <td class="cAPI-table-name">{{name}}</td>
                                                            <td class="cAPI-table-type">{{valueSet}}</td>
                                                            <td class="cAPI-table-desc">{{{description}}}</td>
                                                        </tr>
                                                    {{/if}}  {{/each}}
                                            </table>
                                            <p/><p/>
                                            <!-- Annotations -->
                                            <h2 id="Annotations">
                                                <span class="fw-annotation sm-icon"></span> Annotations
                                            </h2>
                                            <table>
                                                <tr>
                                                    <th class="cAPI-table-name">Name</th>
                                                    <th class="cAPI-table-type">Attachement Points</th>
                                                    <th class="cAPI-table-type">Data Type</th>
                                                    <th class="cAPI-table-desc">Description</th>
                                                </tr>
                                                {{#each constructs}}
                                                    {{#if isAnnotation}}
                                                        <tr>
                                                            <td class="cAPI-table-name">{{name}}</td>
                                                            <td class="cAPI-table-type">{{attachments}}</td>
                                                            <td class="cAPI-table-type"><a href="{{href}}">{{dataType}}</a></td>
                                                            <td class="cAPI-table-desc">{{{description}}}</td>
                                                        </tr>
                                                    {{/if}}  {{/each}}
                                            </table>
                                            <p/><p/>
                                            <!-- Global Variables-->
                                            <h2 id="GlobalVariables">
                                                <span class="fw-globe sm-icon"></span> Global Variables
                                            </h2>
                                            <table>
                                                <tr>
                                                    <th class="cAPI-table-name">Name</th>
                                                    <th class="cAPI-table-type">Data Type</th>
                                                    <th class="cAPI-table-desc">Description</th>
                                                </tr>
                                                {{#each constructs}}
                                                    {{#if isGlobalVariable}}
                                                        <tr>
                                                            <td class="cAPI-table-name">{{name}}</td>
                                                            <td class="cAPI-table-type"><a href="{{href}}">{{dataType}}</a></td>
                                                            <td class="cAPI-table-desc">{{{description}}}</td>
                                                        </tr>
                                                    {{/if}}  {{/each}}
                                            </table>
                                            <p/><p/>
                                        </div>

                                        {{#each constructs}}
                                        <div class="construct-wrapper">
                                            <!--Primitive Types-->
                                            {{#if isPrimitiveType}}
                                                <h2 id="{{name}}" class="cPrimitive">
                                                    <span class="{{icon}} sm-icon"></span> {{name}}
                                                </h2>
                                                {{#if description}}
                                                    <div class="cPrimitiveDescription">
                                                        <span class="fw-right gray xs-icon"></span>{{{description}}}
                                                    </div>
                                                {{/if}} {{#if children}}
                                                <div class="function-wrapper">
                                                    <ul>
                                                        {{#each children}}
                                                            <li class="cFunctionList">
                                                                <h2 id="{{../name}}-{{name}}">
                                                                    <span class={{icon}}></span>
                                                                    <span class="thin-text light-gray">&lt;{{../name}}&gt;</span> {{name}}({{join parameters ", "}}) {{#if returnParams}}returns ({{join returnParams ", "}}) {{/if}}
                                                                </h2>
                                                                {{#if description}}
                                                                    <p>
                                                                        <span class="fw-right gray xs-icon"></span>{{{description}}}
                                                                    </p>
                                                                {{/if}} {{#if parameters}}
                                                                <table>
                                                                    <tr>
                                                                        <th class="cAPI-table-name">Parameter Name</th>
                                                                        <th class="cAPI-table-type">Data Type</th>
                                                                        <th class="cAPI-table-default">Default Value</th>
                                                                        <th class="cAPI-table-desc">Description</th>
                                                                    </tr>
                                                                    {{#each parameters}}
                                                                        <tr>
                                                                            <td class="cAPI-table-name">{{name}}</td>
                                                                            <td class="cAPI-table-type"><a href="{{href}}">{{dataType}}</a></td>
                                                                            <td class="cAPI-table-default"></td>
                                                                            <td class="cAPI-table-desc">{{{description}}}</td>
                                                                        </tr>
                                                                    {{/each}}
                                                                </table>
                                                            {{/if}} {{#if returnParams}}
                                                                <div class="cReturnTableHeader"></div>
                                                                <table>
                                                                    <tr>
                                                                        <th class="cAPI-table-name"></th>
                                                                        <th class="cAPI-table-type">Return Type</th>
                                                                        <th class="cAPI-table-default"></th>
                                                                        <th class="cAPI-table-desc">Description</th>
                                                                    </tr>
                                                                    {{#each returnParams}}
                                                                        <tr>
                                                                            <td class="cAPI-table-name"></td>
                                                                            <td class="cAPI-table-type"><a href="{{href}}">{{dataType}}</a></td>
                                                                            <td class="cAPI-table-default"></td>
                                                                            <td class="cAPI-table-desc">{{{description}}}</td>
                                                                        </tr>
                                                                    {{/each}}
                                                                </table>
                                                            {{/if}}
                                                            </li>
                                                        {{/each}}
                                                    </ul>
                                                </div>
                                            {{/if}} {{/if}}
                                            <!-- Function -->
                                            {{#if isFunction}}
                                                <h2 id="{{name}}">
                                                    <span class="{{icon}} sm-icon"> </span> public function {{name}}({{join parameters ", "}}) {{#if returnParams}}returns ({{join returnParams ", "}}) {{/if}}
                                                </h2>
                                                {{#if description}}
                                                    <p>
                                                        <span class="fw-right gray xs-icon"></span>{{{description}}}
                                                    </p>
                                                {{/if}} {{#if parameters}}
                                                <table>
                                                    <tr>
                                                        <th class="cAPI-table-name">Parameter Name</th>
                                                        <th class="cAPI-table-type">Data Type</th>
                                                        <th class="cAPI-table-default">Default Value</th>
                                                        <th class="cAPI-table-desc">Description</th>
                                                    </tr>
                                                    {{#each parameters}}
                                                        <tr>
                                                            <td class="cAPI-table-name">{{name}}</td>
                                                            <td class="cAPI-table-type"><a href="{{href}}">{{dataType}}</a></td>
                                                            <td class="cAPI-table-default"></td>
                                                            <td class="cAPI-table-desc">{{{description}}}</td>
                                                        </tr>
                                                    {{/each}}
                                                </table>
                                            {{/if}} {{#if returnParams}}
                                                <div class="cReturnTableHeader"></div>
                                                <table>
                                                    <tr>
                                                        <th class="cAPI-table-name"></th>
                                                        <th class="cAPI-table-type">Return Type</th>
                                                        <th class="cAPI-table-default"></th>
                                                        <th class="cAPI-table-desc">Description</th>
                                                    </tr>
