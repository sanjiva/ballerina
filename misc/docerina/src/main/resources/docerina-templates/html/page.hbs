        crossorigin="anonymous">
    <!-- Latest compiled and minified JavaScript -->
    <script src="//maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa"
        crossorigin="anonymous"></script>

    <!-- Nanoscroller JS -->
    <script src="docerina-theme/js/jquery.nanoscroller.min.js"></script>

    <!-- Docerina JS -->
    <script src="docerina-theme/js/docerina.js"></script>
    <script type="text/javascript" src="/js/highlight.pack.js"></script>
    <link rel="stylesheet" href="//use.typekit.net/son4ymv.css" />
