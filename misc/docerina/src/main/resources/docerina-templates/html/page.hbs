<body class="cBallerina-io" role="document">
    <div class="row cBallerina-io-top-row">
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6"></div>
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 cTopLineContainer">
            <div class="cTopLine"></div>
        </div>
    </div>
    <div class="row cBallerina-io-Nav" id="iMainNavigation">
    </div>
    <div class="row cBallerina-io-Logo-row">
        <div class="container">
            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 cBallerina-io-Logo-Container">
                <a class="cLogo" href="/">
                    <img src="//b.content.wso2.com/sites/all/ballerina.io-cdn/img/ballerina-logo.svg" alt="Ballerina" />
                </a>
            </div>
        </div>
    </div>

    <div class="row cBallerina-io-Gray-row">
        <div class="container">
            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-3 cBallerina-io-Home-Left-col">
                <div class="cBlallerina-io-docs-left-nav-container">
                    <div class="cSideBar-Button">
                        <img src="//b.content.wso2.com/sites/all/ballerina.io-cdn/img/sidebar.svg" />
                    </div>
                    <div class="cLectColStyle">
                        <div class="cBallerina-io-docs-Title">TABLE OF CONTENTS</div>
                        {{#each links}}
                        <ul class=" {{#if active}}active-package{{/if}}">
                            <li>
                                <!--Package Name-->
                                {{#if active}} {{#if caption.isPackageName}}
                                <a>{{caption.prefix}}
                                    <span class="bold">{{caption.suffix}}</a>
                                {{/if}} {{#if caption.isStaticCaption}}
                                <a>
                                    <span class="bold">{{caption.value}}</span>
                                </a>
                                {{/if}}
                                <div class="constructs-wrapper">
                                    <!--Construct of Package-->
                                    <ul class="constructs-ul">
                                        {{#each constructs}}
                                        <li>
                                            <a href="#{{name}}">
                                                <div class="construct-icon">
                                                    <span class="{{icon}}"> </span>
                                                </div>
                                                <div class="construct-name">
                                                    {{name}} 
                                                    {{#if isConnector}} ({{#each parameters}}{{dataType}} {{name}}{{#unless
                                                    @last}} ,{{/unless}}{{/each}}) {{/if}}
                                                </div>
                                            </a>
                                        </li>
                                        {{#each children}}
                                        <ul class="construct-inner-items-ul">
                                            <li>
                                                <a href="#{{../name}}-{{name}}">
                                                    <div class="construct-icon">
                                                        <span class="{{icon}}"> </span>
                                                    </div>
                                                    <div class="construct-name">
                                                        {{name}}
                                                    </div>
                                                </a>
                                            </li>
                                        </ul>
                                        {{/each}} {{/each}}
                                    </ul>
                                </div>
                                {{else}} {{!--
                                <a href="{{href}}.html">
                                    {{#if caption.isPackageName}} {{caption.prefix}}
                                    <span class="bold">{{caption.suffix}}</span>
                                    {{/if}} {{#if caption.isStaticCaption}}
                                    <span class="bold">{{caption.value}}</span>
                                    {{/if}}
                                </a> --}} {{/if}}
                            </li>
                        </ul>
                        {{/each}}





                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 cBallerina-io-Home-Middle-col">
                <div class="cBallerina-io-breadcrumbs">
                <div role="navigation" aria-label="breadcrumbs navigation">
                <ul class="wy-breadcrumbs">
                <li>
                                        <a href="/">Home</a>
                                    </li>
                                    <li>»</li>
                                    <li>
                                        <a href="/learn">Learn</a>
                                    </li>
                                    <li>»</li>
                                    <li>{{name}}</li>
                </ul>
                </div>
                </div>


                <div class="cBlallerina-io-docs-content-container">
                    <div>
                        <div>

                            <div role="main">
                                <div>
                                    <div id="content" class="nano">
                                        <div class="nano-content">
                                            <h1 class="thin-text padding-bottom">
                                                <span class="bold">{{heading.value}}</span>
                                                {{#if heading.isPackageName}} package {{/if}}
                                            </h1>
                                            {{#each constructs}}
                                            <div class="construct-wrapper">
                                                <!--Primitive Types-->
                                                {{#if isPrimitiveType}}
                                                <h2 id="{{name}}">
                                                    <span class="{{icon}} sm-icon"></span>
                                                    {{name}}
                                                </h2>
                                                {{#if description}}
                                                <p>
                                                    <span class="fw-right gray xs-icon"></span>{{description}}
                                                </p>
                                                {{/if}} {{#if children}}
                                                <div class="function-wrapper">
                                                    <ul>
                                                        {{#each children}}
                                                        <li>
                                                            <h2 id="{{../name}}-{{name}}">
                                                                <span class={{icon}}></span>
                                                                <span class="thin-text light-gray">&lt; {{../name}} &gt;</span>
                                                                {{name}}({{join parameters ", "}}) {{#if returnParams}}returns ({{join returnParams ", "}}) {{/if}}
                                                            </h2>
                                                            {{#if description}}
                                                            <p>
                                                                <span class="fw-right gray xs-icon"></span>{{description}}
                                                            </p>
                                                            {{/if}} {{#if parameters}}
                                                            <p>
                                                                <label class="table-header-text">Parameters:</label>
                                                            </p>
                                                            <table>
                                                                <tr>
                                                                    <th class="col-md-3">Parameter Name</th>
                                                                    <th class="col-md-2">Data Type</th>
                                                                    <th class="col-md-7">Description</th>
                                                                </tr>
                                                                {{#each parameters}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                                {{/each}}
                                                            </table>
                                                            {{/if}} {{#if returnParams}}
                                                            <table>
                                                                <tr>
                                                                    <th class="col-md-3">Return Variable</th>
                                                                    <th class="col-md-2">Data Type</th>
                                                                    <th class="col-md-7">Description</th>
                                                                </tr>
                                                                {{#each returnParams}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                                {{/each}}
                                                            </table>
                                                            {{/if}}
                                                        </li>
                                                        {{/each}}
                                                    </ul>
                                                </div>
                                                {{/if}} {{/if}}
                                                <!-- Function -->
                                                {{#if isFunction}}
                                                <h2 id="{{name}}">
                                                    <span class="{{icon}} sm-icon"> </span>
                                                    public function {{name}}({{join parameters ", "}}) {{#if returnParams}}returns ({{join returnParams ", "}}) {{/if}}
                                                </h2>
                                                {{#if description}}
                                                <p>
                                                    <span class="fw-right gray xs-icon"></span>{{description}}
                                                </p>
                                                {{/if}} {{#if parameters}}
                                                <p>
                                                    <label class="table-header-text">Parameters:</label>
                                                </p>
                                                <table>
                                                    <tr>
                                                        <th class="col-md-3">Parameter Name</th>
                                                        <th class="col-md-2">Data Type</th>
                                                        <th class="col-md-7">Description</th>
                                                    </tr>
                                                    {{#each parameters}}
                                                    <tr>
                                                        <td class="col-md-3">{{name}}</td>
                                                        <td class="col-md-2">{{dataType}}</td>
                                                        <td class="col-md-7">{{description}}</td>
                                                    </tr>
                                                    {{/each}}
                                                </table>
                                                {{/if}} {{#if returnParams}}
                                                <p>
                                                    <label class="table-header-text">Return Parameters:</label>
                                                </p>
                                                <table>
                                                    <tr>
                                                        <th class="col-md-3">Return Variable</th>
                                                        <th class="col-md-2">Data Type</th>
                                                        <th class="col-md-7">Description</th>
                                                    </tr>
                                                    {{#each returnParams}}
                                                    <tr>
                                                        <td class="col-md-3">{{name}}</td>
                                                        <td class="col-md-2">{{dataType}} </td>
                                                        <td class="col-md-7">{{description}}</td>
                                                    </tr>
                                                    {{/each}}
                                                </table>
                                                {{/if}} {{/if}}
                                                <!-- Struct-binded functions grouped together -->
                                                {{#if isStruct}}
                                                <h2 id="{{name}}">
                                                    <span class="{{icon}} sm-icon"></span>
                                                    public struct {{name}}
                                                </h2>
                                                {{#if description}}
                                                <p>
                                                    <span class="fw-right gray xs-icon"></span>{{description}}
                                                </p>
                                                {{/if}} {{#if fields}}
                                                <p>
                                                    <label class="table-header-text">Fields:</label>
                                                </p>
                                                <table>
                                                    <tr>
                                                        <th class="col-md-3">Field Name</th>
                                                        <th class="col-md-2">Data Type</th>
                                                        <th class="col-md-5">Description</th>
                                                        <th class="col-md-2">Default Value</th>
                                                    </tr>
                                                    {{#each fields}}
                                                    <tr>
                                                        <td class="col-md-3">{{name}}</td>
                                                        <td class="col-md-2">{{dataType}}</td>
                                                        <td class="col-md-5">{{description}}</td>
                                                        <td class="col-md-2">{{defaultValue}}</td>
                                                    </tr>
                                                    {{/each}}
                                                </table>
                                                {{/if}} {{#if children}}
                                                <div class="function-wrapper">
                                                    <ul>
                                                        {{#each children}}
                                                        <li>
                                                            <h2 id="{{../name}}-{{name}}">
                                                                <span class={{icon}}></span>
                                                                <span class="thin-text light-gray">&lt;{{../name}}&gt;</span>
                                                                {{name}}({{join parameters ", "}}) {{#if returnParams}}returns ({{join returnParams ", "}}) {{/if}}
                                                            </h2>
                                                            {{#if description}}
                                                            <p>
                                                                <span class="fw-right gray xs-icon"></span>{{description}}
                                                            </p>
                                                            {{/if}} {{#if parameters}}
                                                            <p>
                                                                <label class="table-header-text">Parameters:</label>
                                                            </p>
                                                            <table>
                                                                <tr>
                                                                    <th class="col-md-3">Parameter Name</th>
                                                                    <th class="col-md-2">Data Type</th>
                                                                    <th class="col-md-7">Description</th>
                                                                </tr>
                                                                {{#each parameters}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                                {{/each}}
                                                            </table>
                                                            {{/if}} {{#if returnParams}}
                                                            <p>
                                                                <label class="table-header-text">Return Parameters:</label>
                                                            </p>
                                                            <table>
                                                                <tr>
                                                                    <th class="col-md-3">Return Variable</th>
                                                                    <th class="col-md-2">Data Type</th>
                                                                    <th class="col-md-7">Description</th>
                                                                </tr>
                                                                {{#each returnParams}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                                {{/each}}
                                                            </table>
                                                            {{/if}}
                                                        </li>
                                                        {{/each}}
                                                    </ul>
                                                </div>
                                                {{/if}} {{/if}}
                                                <!-- Connectors and its actions -->
                                                {{#if isConnector}}
                                                <h2 id="{{name}}">
                                                    <span class="{{icon}} sm-icon"></span>
                                                    public connector {{name}}({{join parameters ", "}})
                                                </h2>
                                                {{#if description}}
                                                <p>
                                                    <span class="fw-right gray xs-icon"></span>{{description}}
                                                </p>
                                                {{/if}} {{#if parameters}}
                                                <p>
                                                    <label class="table-header-text">Parameters:</label>
                                                </p>
                                                <table>
                                                    <tr>
                                                        <th class="col-md-4">Parameter Name</th>
                                                        <th class="col-md-2">Data Type</th>
                                                        <th class="col-md-6">Description</th>
                                                    </tr>
                                                    {{#each parameters}}
                                                    <tr>
                                                        <td class="col-md-3">{{name}}</td>
                                                        <td class="col-md-2">{{dataType}}</td>
                                                        <td class="col-md-7">{{description}}</td>
                                                    </tr>
                                                    {{/each}}
                                                </table>
                                                {{/if}} {{#if children}}
                                                <div class="function-wrapper">
                                                    <h2>Actions:</h2>
                                                    <ul>
                                                        {{#each children}}
                                                        <li>
                                                            <h2 id="{{../name}}-{{name}}">
                                                                <span class={{icon}}></span>
                                                                <span class="thin-text light-gray">
                                                                    <{{../name}}>
                                                                </span>
                                                                {{name}}({{join parameters ", "}}) {{#if returnParams}}returns ({{join returnParams ", "}}) {{/if}}
                                                            </h2>
                                                            {{#if description}}
                                                            <p>
                                                                <span class="fw-right gray xs-icon"></span>{{description}}
                                                            </p>
                                                            {{/if}} {{#if parameters}}
                                                            <p>
                                                                <label class="table-header-text">Parameters:</label>
                                                            </p>
                                                            <table>
                                                                <tr>
                                                                    <th class="col-md-3">Parameter Name</th>
                                                                    <th class="col-md-2">Data Type</th>
                                                                    <th class="col-md-7">Description</th>
                                                                </tr>
                                                                {{#each parameters}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                                {{/each}}
                                                            </table>
                                                            {{/if}} {{#if returnParams}}
                                                            <p>
                                                                <label class="table-header-text">Return Parameters:</label>
                                                            </p>
                                                            <table>
                                                                <tr>
                                                                    <th class="col-md-3">Return Variable</th>
                                                                    <th class="col-md-2">Data Type</th>
                                                                    <th class="col-md-7">Description</th>
                                                                </tr>
                                                                {{#each returnParams}}
                                                                <tr>
                                                                    <td class="col-md-3">{{name}}</td>
                                                                    <td class="col-md-2">{{dataType}}</td>
                                                                    <td class="col-md-7">{{description}}</td>
                                                                </tr>
                                                                {{/each}}
                                                            </table>
                                                            {{/if}}
                                                        </li>
                                                        {{/each}}
                                                    </ul>
                                                </div>
                                                {{/if}} {{/if}}
                                                <!-- Enums-->
                                                {{#if isEnum}}
                                                <h2 id="{{name}}">
                                                    <span class="{{icon}} sm-icon"></span>
                                                    public enum {{name}}
                                                </h2>
                                                {{#if description}}
                                                <p>
                                                    <span class="fw-right gray xs-icon"></span>{{description}}
                                                </p>
                                                {{/if}} {{#if enumerators}}
                                                <p>
                                                    <label class="table-header-text">Enumerators:</label>
                                                </p>
                                                <table>
                                                    <tr>
                                                        <th class="col-md-3">Name</th>
                                                        <th class="col-md-9">Description</th>
                                                    </tr>
                                                    {{#each enumerators}}
                                                    <tr>
                                                        <td class="col-md-3">{{name}}</td>
                                                        <td class="col-md-9">{{description}}</td>
                                                    </tr>
                                                    {{/each}}
                                                </table>
                                                {{/if}} {{/if}}
                                                <!-- Annotations-->
                                                {{#if isAnnotation}}
                                                <h2 id="{{name}}">
                                                    <span class="{{icon}} sm-icon"></span>
                                                    public annotation {{name}}
                                                </h2>
                                                {{#if description}}
                                                <p>
                                                    <span class="fw-right gray xs-icon"></span>{{description}}
                                                </p>
                                                {{/if}} {{#if attributes}}
                                                <p>
                                                    <label class="table-header-text">Attributes:</label>
                                                </p>
                                                <table>
                                                    <tr>
                                                        <th class="col-md-3">Attribute Name</th>
                                                        <th class="col-md-2">Data Type</th>
                                                        <th class="col-md-7">Description</th>
                                                    </tr>
                                                    {{#each attributes}}
                                                    <tr>
                                                        <td class="col-md-3">{{name}}</td>
                                                        <td class="col-md-2">{{dataType}}</td>
                                                        <td class="col-md-7">{{description}}</td>
                                                    </tr>
                                                    {{/each}}
                                                </table>
                                                {{/if}} {{/if}}
                                                <!-- Global Variables-->
                                                {{#if isGlobalVariable}}
                                                <h2 id="{{name}}">
                                                    <span class="{{icon}} sm-icon"></span>
                                                    {{name}}
                                                </h2>
                                                <table>
                                                    <tr>
                                                        <th class="col-md-3">Attribute Name</th>
                                                        <th class="col-md-2">Data Type</th>
                                                        <th class="col-md-7">Description</th>
                                                    </tr>
                                                    <tr>
                                                        <td class="col-md-3">{{name}}</td>
                                                        <th class="col-md-2">{{dataType}}</th>
                                                        <td class="col-md-7">{{description}}</td>
                                                    </tr>
                                                </table>
                                                {{/if}}
                                            </div>
                                            {{/each}}
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
            <div class="col-xs-12 col-sm-12 col-md-6 col-lg-3 cBallerina-io-Home-Right-col">
                <div class="cBlallerina-io-docs-right-nav-container">
                    <div class="cSideBar-Button">
                        <img src="//b.content.wso2.com/sites/all/ballerina.io-cdn/img/sidebar.svg" />
                    </div>
                    <div class="cLectColStyle">
                        {{!-- <div class="cBallerina-io-docs-Title">PRIMITIVE TYPES</div> --}}
                        <div class="cBallerina-io-docs-Title">BALLERINA PACKAGES</div>
                        {{#each links}}
                        <ul class=" {{#if active}}active-package{{/if}}">
                            <li>
                                <!--Package Name-->
                                {{#if active}} {{#if caption.isPackageName}}
                                <a>{{caption.prefix}}
                                    <span class="bold">{{caption.suffix}}</a>
                                {{/if}} {{#if caption.isStaticCaption}}
                                <a>
                                    <span class="bold">{{caption.value}}</span>
                                </a>
                                {{/if}}
                                <div class="constructs-wrapper">
                                    <!--Construct of Package-->
                                    <ul class="constructs-ul">
                                        {{#each constructs}} {{!--
                                        <li>
                                            <a href="#{{name}}">
                                                <div class="construct-icon">
                                                    <span class="{{icon}}"> </span>
                                                </div>
                                                <div class="construct-name">
                                                    {{name}} {{#if isFunction}}({{join parameters ", "}}) {{#if returnParams}} ({{join returnParams ", "}}) {{/if}} {{/if}}
                                                    {{#if isConnector}} ({{#each parameters}}{{dataType}} {{name}}{{#unless
                                                    @last}} ,{{/unless}}{{/each}}) {{/if}}
                                                </div>
                                            </a>
                                        </li> --}} {{!-- {{#each children}}
                                        <ul class="construct-inner-items-ul">
                                            <li>
                                                <a href="#{{../name}}-{{name}}">
                                                    <div class="construct-icon">
                                                        <span class="{{icon}}"> </span>
                                                    </div>
                                                    <div class="construct-name">
                                                        {{name}}({{join parameters ", "}}) {{#if returnParams}} ({{join returnParams ", "}}) {{/if}}
                                                    </div>
                                                </a>
                                            </li>
                                        </ul>
                                        {{/each}} --}} {{/each}}
                                    </ul>
                                </div>
                                {{else}}
                                <a href="{{href}}.html">
                                    {{#if caption.isPackageName}} {{caption.prefix}}
                                    <span class="bold">{{caption.suffix}}</span>
                                    {{/if}} {{#if caption.isStaticCaption}}
                                    <span class="bold">{{caption.value}}</span>
                                    {{/if}}
                                </a>
                                {{/if}}
                            </li>
                        </ul>
                        {{/each}}





                    </div>
                </div>
            </div>
        </div>
    </div>
    <div id="iBallerinaFooter" class="row cBallerina-io-White-row">
 
    </div>



</body>

</html>
