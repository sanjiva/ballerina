<div class="row cBallerina-io-top-row">
    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6"></div>
    <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 cTopLineContainer">
        <div class="cTopLine"></div>
    </div>
</div>
<div class="row cBallerina-io-Nav" id="iMainNavigation">
</div>
<div class="row cBallerina-io-Logo-row">
    <div class="container">
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-6 cBallerina-io-Logo-Container">
            <a class="cLogo" href="/">
                <img src="/img/ballerina-logo.png" alt="Ballerina" />
            </a>
        </div>
    </div>
</div>
<div class="row cBallerina-io-Gray-row">
    <div class="container">
        <div class="col-xs-12 col-sm-12 col-md-3 col-lg-3 cBallerina-io-Home-Left-col">
            <div class="cBlallerina-io-docs-right-nav-container">
                <div class="cLectColStyle cLeftColTop">
                    <div class="cBallerina-io-docs-Title cBallerina-io-primitive-types">PRIMITIVE TYPES</div>
                    <div class="cCollaps-Menu-first">
                        {{#each primitives}}
                            <ul class="">
                                <li>
                                    <a href="{{href}}">
                                        <span class="bold">{{caption.value}}</span>
                                    </a>
                                </li>
                            </ul>
                        {{/each}}
                    </div>
                </div>
            </div>
            <div class="cBlallerina-io-docs-right-nav-container">
                <div class="cLectColStyle cLeftColTop">
                    <div class="cBallerina-io-docs-Title cBallerina-io-packages">BALLERINA PACKAGES</div>

                    <div class="cCollaps-Menu" id="iBallerinaPACKAGEStoc">


                    </div>
                </div>
            </div>
            <div class="cBlallerina-io-docs-left-nav-container">
                <div class="cSideBar-Button">
                    <img src="/img/sidebar.svg" />
                </div>
                <div class="cLectColStyle">
                    <div class="cBallerina-io-docs-Title">TABLE OF CONTENTS</div>
                    {{#each links}}
                        <ul class=" {{#if active}}active-package{{/if}}">
                            <li>
                                <!--Package Name-->
                                {{#if active}} {{#if caption.isPackageName}}
                                    <a class="cPackageList">{{caption.prefix}}<span class="bold">{{caption.suffix}}</a> {{/if}} {{#if caption.isStaticCaption}}
                                    <a>
                                        <span class="bold">{{caption.value}}</span>
                                    </a>
                                {{/if}}
                                    <div class="constructs-wrapper">
                                        <!--Construct of Package-->
                                        <ul class="constructs-ul">
                                            {{#each constructs}}
                                                {{#unless isGlobalVariable}} {{#unless isType}} {{#unless isAnnotation}}
                                                    <li>
                                                        <a href="#{{name}}">
                                                            <div class="construct-icon">
                                                                <span class="{{icon}}"> </span>
                                                            </div>
                                                            <div class="construct-name">
                                                                {{name}} {{#if isConnector}} ({{#each fields}}<a href="{{href}}">{{dataType}}</a> {{name}}{{#unless @last}} ,{{/unless}}{{/each}}) {{/if}}
                                                            </div>
                                                        </a>
                                                    </li>
                                                    {{#each children}}
                                                        <ul class="construct-inner-items-ul">
                                                            <li>
                                                                <a href="#{{../name}}-{{name}}">
                                                                    <div class="construct-icon">
                                                                        <span class="{{icon}}"> </span>
                                                                    </div>
                                                                    <div class="construct-name">
                                                                        {{name}}
                                                                    </div>
                                                                </a>
                                                            </li>
                                                        </ul>
                                                    {{/each}} {{/unless}} {{/unless}} {{/unless}} {{/each}}
                                            <li>
                                                <a href="#TypeDefinitions">
                                                    <div class="construct-icon">
                                                        <span class="fw-constant"> </span>
                                                    </div>
                                                    <div class="construct-name">
                                                        Type Definitions
                                                    </div>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="#Annotations">
                                                    <div class="construct-icon">
                                                        <span class="fw-annotation"> </span>
                                                    </div>
                                                    <div class="construct-name">
                                                        Annotations
                                                    </div>
                                                </a>
                                            </li>
                                            <li>
                                                <a href="#GlobalVariables">
                                                    <div class="construct-icon">
                                                        <span class="fw-globe"> </span>
                                                    </div>
                                                    <div class="construct-name">
                                                        Global Variables
                                                    </div>
                                                </a>
                                            </li>
                                        </ul>
                                    </div>
                                {{else}} {{!--
                                <a href="{{href}}.html">
                           {{#if caption.isPackageName}} {{caption.prefix}}
                           <span class="bold">{{caption.suffix}}</span>
                           {{/if}} {{#if caption.isStaticCaption}}
                           <span class="bold">{{caption.value}}</span>
                           {{/if}}
                           </a> --}} {{/if}}
                            </li>
                        </ul>
                    {{/each}}
                </div>
            </div>
        </div>
        <div class="col-xs-12 col-sm-12 col-md-6 col-lg-9 cBallerina-io-Home-Middle-col">
