                    <ul class="wy-breadcrumbs">
                        <li>
                            <a href="/">Home</a>
                        </li>
                        <li>»</li>
                        <li>
                            <a href="/learn">Learn</a>
                        </li>
                        <li>»</li>
                        <li>
                            <a href="/learn/api-docs/">API Docs</a>
                        </li>
                        <li>»</li>
                        <li><span class="bold">{{heading.value}}</span>{{#if heading.isPackageName}} package {{/if}}</li>
                    </ul>
                </div>
            </div>
            <div class="cBlallerina-io-docs-content-container">
                <div>
                    <div>
                        <div role="main">
                            <div>
                                <div id="content" class="nano">
                                    <div class="nano-content">
                                        <h1 class="thin-text padding-bottom">
                                            <span class="bold">{{heading.value}}</span> {{#if heading.isPackageName}} package {{/if}}
                                        </h1>
                                        {{#if description}}
                                            <div class="cAPIdocsDescription">
                                                {{{description}}}
                                            </div>
                                        {{/if}}

                                        <div class="summary-wrapper">
                                            <!--Primitive Types-->
                                            <table>
                                                <caption>Primitives Summary</caption>
                                                <tr>
                                                    <th class="cSummary-table-type">Type</th>
                                                    <th class="cSummary-table-desc">Description</th>
                                                </tr>
                                                {{#each constructs}}
                                                    {{#if isPrimitiveType}}
                                                        <tr>
                                                            <td class="cSummary-table-type"><a href="#{{name}}">{{name}}</td>
                                                            <td class="cSummary-table-desc">{{{description}}}</td>
                                                        </tr>
                                                    {{/if}}
                                                {{/each}}
                                            </table>
                                            <p/><p/>
                                            <table>
                                                <caption>Objects Summary</caption>
                                                <tr>
                                                    <th class="cSummary-table-type">Object</th>
                                                    <th class="cSummary-table-desc">Description</th>
                                                </tr>
                                                {{#each constructs}}
                                                    {{#if isStruct}}
                                                        <tr>
                                                            <td class="cSummary-table-type"><a href="#{{name}}">{{name}}</a></td>
                                                            <td class="cSummary-table-desc">{{{description}}}</td>
                                                        </tr>
                                                    {{/if}}
                                                {{/each}}
                                            </table>
                                            <p/><p/>
